<!DOCTYPE html>
<html lang="hebrew">
<head>
    <script src="https://code.jquery.com/jquery-3.1.0.js"   integrity="sha256-slogkvB1K3VOkzAI8QITxV3VzpOnkeNVsKvtkYLMjfk="   crossorigin="anonymous"></script>
    <script src="https://rawgit.com/jackmoore/autosize/master/dist/autosize.min.js"></script>
    <script src="/assets/js/bootstrap.js"></script>
    <script src="/assets/js/jscolor.js"></script>
    <script src="/assets/js/words.js"></script>
    <script src="/assets/js/functionality.js"></script>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link id="style" rel="stylesheet" type="text/css" href="/assets/css/bootstrap.css">
    <link id="style" rel="stylesheet" type="text/css" href="/assets/css/style.css">

    <title>TextNinja</title>
</head>
<body>
    <% include partials/navbar.ejs%>
    <% include partials/menu.ejs%>


    <div id="render-div">
        <% if(typeof pageID != 'undefined'){
            switch (pageID) {
            case 0: %>
                <% include pages/home.ejs%>
                <% break;
            case 1: %>
                <% include pages/about.ejs%>
                <% break;
            case 2: %>
                <% include pages/contact.ejs%>
                <% break;
            case 3:
                if(typeof user !== 'undefined'){%>
                    <% include pages/practice.ejs%>
                <%}else {%>
                    <% include pages/home.ejs%>
                <%}%>
                <% break;
            default: %>
                <% include pages/home.ejs%>
                <% break;
            } %>
        <% } %>
    </div>
    <% include partials/sound.ejs%>
    <% include partials/bott.ejs %>
</body>
</html>
